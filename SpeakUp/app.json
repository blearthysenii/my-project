{
  "expo": {
    "name": "SpeakUp",
    "slug": "SpeakUp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "speakup",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.blearthyseni1.SpeakUp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "jsEngine": "jsc",
    "extra": {
      "eas": {
        "projectId": "06551177-d09f-407f-a83a-43aea1a6be30"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/06551177-d09f-407f-a83a-43aea1a6be30"
    },
    "android": {
      "package": "com.blearthyseni1.SpeakUp"
    }
  },
  "android": {
    "adaptiveIcon": {
      "foregroundImage": "./assets/images/adaptive-icon.png",
      "backgroundColor": "#ffffff"
    },
    "edgeToEdgeEnabled": true
  },
  "web": {
    "bundler": "metro",
    "output": "static",
    "favicon": "./assets/images/favicon.png"
  },
  "plugins": [
    "expo-router",
    [
      "expo-splash-screen",
      {
        "image": "./assets/images/splash-icon.png",
        "imageWidth": 200,
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    ]
  ],
  "experiments": {
    "typedRoutes": true
  }
}
